<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileFragment"
    android:backgroundTint="#FFFFFF">

    <RelativeLayout
        android:id="@+id/relativeLayout7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="@dimen/_20sdp">
        <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="@dimen/_20sdp"
            android:src="@drawable/baseline_arrow_back_ios_24"
            android:backgroundTint="#EBEAEA"
            android:scaleType="centerInside"
            android:background="@drawable/round_red"
            android:paddingEnd="-0.5dp"
            android:paddingStart="@dimen/_10sdp"
            android:padding="@dimen/_5sdp"
/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:fontFamily="@font/aclonica"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold"
            android:text="Profile" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout7"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/_30sdp"
        android:orientation="vertical"
        android:gravity="center">

        <ImageView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/bus" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Thokala Harish"
            android:textStyle="bold"
            android:textSize="@dimen/_15sdp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="23AG1A6959"/>

        <androidx.appcompat.widget.AppCompatButton
            android:layout_width="wrap_content"
            android:layout_height="@dimen/_30sdp"
            android:backgroundTint="#0048FF"
            android:textColor="@color/white"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/round_for_edit"
            android:paddingEnd="@dimen/_20sdp"
            android:paddingStart="@dimen/_20sdp"
            android:text="Edit Profile"/>


    </LinearLayout>


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_20sdp"
        android:layout_below="@+id/linearLayout4"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/_30sdp"
        android:layout_marginEnd="@dimen/_30sdp"
        android:scaleType="center"
        android:layout_marginTop="@dimen/_30sdp"
        android:src="@drawable/line"
        app:tint="#6A6868" />

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView3"
        android:layout_centerHorizontal="true"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15sdp">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:src="@drawable/baseline_settings_24"
                android:background="#E8F0F2"
                android:padding="@dimen/_3sdp"
                android:layout_marginStart="@dimen/_40sdp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginStart="@dimen/_18sdp"

                android:layout_toEndOf="@+id/imageView6"
                android:text="Settings"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />

            <ImageButton
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/_50sdp"
                android:rotation="180"
                android:layout_centerVertical="true"
                android:paddingEnd="-5dp"
                android:scaleType="fitCenter"
                android:padding="@dimen/_2sdp"
                android:backgroundTint="#EBEAEA"
                android:background="@drawable/round_red"
                android:src="@drawable/baseline_arrow_back_ios_24" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_22sdp">

            <ImageView
                android:id="@+id/imageVi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"

                android:background="#E8F0F2"
                android:padding="@dimen/_3sdp"
                android:src="@drawable/baseline_discount_24"
                android:layout_marginStart="@dimen/_40sdp"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"

                android:layout_marginStart="@dimen/_18sdp"


                android:layout_toEndOf="@+id/imageVi"
                android:text="Pass Details"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />


            <ImageButton
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/_50sdp"
                android:rotation="180"
                android:layout_centerVertical="true"
                android:paddingEnd="-8dp"
                android:scaleType="fitCenter"
                android:padding="@dimen/_2sdp"
                android:backgroundTint="#EBEAEA"
                android:background="@drawable/round_red"
                android:src="@drawable/baseline_arrow_back_ios_24" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_22sdp">

            <ImageView
                android:id="@+id/imageV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:background="#E8F0F2"
                android:padding="@dimen/_3sdp"

                android:src="@drawable/baseline_desktop_windows_24"
                android:layout_marginStart="@dimen/_40sdp"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"

                android:layout_marginStart="18dp"

                android:layout_toEndOf="@+id/imageV"

                android:text="User Management"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />

            <ImageButton
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/_50sdp"
                android:rotation="180"
                android:layout_centerVertical="true"
                android:paddingEnd="-8dp"
                android:scaleType="fitCenter"
                android:padding="@dimen/_2sdp"
                android:backgroundTint="#EBEAEA"
                android:background="@drawable/round_red"
                android:src="@drawable/baseline_arrow_back_ios_24" />
        </RelativeLayout>


    </LinearLayout>


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_20sdp"
        android:layout_below="@+id/linearLayout6"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/_30sdp"
        android:layout_marginEnd="@dimen/_30sdp"
        android:layout_marginTop="@dimen/_10sdp"
        android:backgroundTint="@color/white"
        android:scaleType="center"
        android:src="@drawable/line"
        app:tint="#6A6868" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView5"
        android:layout_centerHorizontal="true"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_17sdp">

            <ImageView
                android:id="@+id/image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:background="#E8F0F2"
                android:padding="@dimen/_3sdp"

                android:src="@drawable/baseline_info_24"
                android:layout_marginStart="@dimen/_40sdp"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"

                android:layout_marginStart="18dp"

                android:layout_toEndOf="@+id/image"

                android:text="Information"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />

            <ImageButton
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/_50sdp"
                android:rotation="180"
                android:layout_centerVertical="true"
                android:paddingEnd="-8dp"
                android:scaleType="fitCenter"
                android:padding="@dimen/_2sdp"
                android:backgroundTint="#EBEAEA"
                android:background="@drawable/round_red"
                android:src="@drawable/baseline_arrow_back_ios_24" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_22sdp">

            <ImageView
                android:id="@+id/imag"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:background="#E8F0F2"
                android:padding="@dimen/_3sdp"

                android:src="@drawable/baseline_logout_24_2"
                android:layout_marginStart="@dimen/_40sdp"/>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"

                android:layout_marginStart="15dp"
                android:layout_toEndOf="@+id/imag"

                android:text="Logout"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />

            <ImageButton
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="@dimen/_50sdp"
                android:rotation="180"
                android:layout_centerVertical="true"
                android:paddingEnd="-8dp"
                android:scaleType="fitCenter"
                android:padding="@dimen/_2sdp"
                android:backgroundTint="#EBEAEA"
                android:background="@drawable/round_red"
                android:src="@drawable/baseline_arrow_back_ios_24" />
        </RelativeLayout>
    </LinearLayout>

</RelativeLayout>